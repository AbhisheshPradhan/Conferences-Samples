{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "first-tab-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/firstTab/FirstTabWebPart.js",
          "manifest": "./src/webparts/firstTab/FirstTabWebPart.manifest.json"
        }
      ]
    },
    "second-tab-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/secondTab/SecondTabWebPart.js",
          "manifest": "./src/webparts/secondTab/SecondTabWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "FirstTabWebPartStrings": "lib/webparts/firstTab/loc/{locale}.js",
    "SecondTabWebPartStrings": "lib/webparts/secondTab/loc/{locale}.js"
  }
}
